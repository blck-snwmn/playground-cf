		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240925.0",
		"@remix-run/dev": "^2.12.1",
		"@remix-run/node": "^2.12.1",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^8.7.0",
		"eslint": "^9.11.1",
