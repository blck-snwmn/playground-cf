		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240620.0",
		"@remix-run/dev": "^2.10.0",
		"@remix-run/node": "^2.10.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"eslint": "^9.6.0",
